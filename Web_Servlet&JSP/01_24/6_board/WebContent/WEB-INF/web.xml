  </servlet>
  <servlet-mapping>
    <servlet-name>AddBoardServlet</servlet-name>
    <url-pattern>/addBoard.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GetBoardServlet</display-name>
    <servlet-name>GetBoardServlet</servlet-name>
